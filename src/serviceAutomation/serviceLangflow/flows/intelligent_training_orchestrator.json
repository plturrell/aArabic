{
  "name": "Intelligent Multi-Model Training Orchestrator",
  "description": "Advanced training with Tencent HY-MT, M2M100, RLM recursive learning, and ToolOrchestra intelligence",
  "version": "2.0.0",
  "nodes": [
    {
      "id": "model_selector",
      "type": "CustomComponent",
      "name": "Multi-Model Selector",
      "position": {"x": 100, "y": 100},
      "data": {
        "component": "MultiModelSelector",
        "description": "Select and configure multiple models for comparison",
        "inputs": {
          "models": {
            "type": "list",
            "options": [
              "m2m100-418M",
              "tencent-hy-mt-1.8B",
              "custom-ensemble"
            ],
            "default": ["m2m100-418M", "tencent-hy-mt-1.8B"]
          },
          "enable_comparison": {
            "type": "bool",
            "default": true
          }
        },
        "outputs": {
          "model_configs": "ModelConfig[]",
          "comparison_enabled": "bool"
        }
      }
    },
    {
      "id": "tencent_hy_mt",
      "type": "CustomComponent",
      "name": "Tencent HY-MT 1.8B Loader",
      "position": {"x": 100, "y": 250},
      "data": {
        "component": "TencentHYMTLoader",
        "description": "Load Tencent HY-MT 1.8B translation model",
        "inputs": {
          "model_path": {
            "type": "str",
            "default": "../../vendor/layerModels/huggingFace/tencent/HY-MT1.5-1.8B"
          },
          "device": {
            "type": "str",
            "options": ["cpu", "cuda", "metal"],
            "default": "cpu"
          },
          "load_in_8bit": {
            "type": "bool",
            "default": false
          }
        },
        "outputs": {
          "model": "TencentModel",
          "config": "ModelConfig",
          "parameters": "int"
        }
      }
    },
    {
      "id": "m2m100_loader",
      "type": "CustomComponent",
      "name": "M2M100 418M Loader",
      "position": {"x": 100, "y": 400},
      "data": {
        "component": "M2M100Loader",
        "description": "Load M2M100 with 99.9% weights",
        "inputs": {
          "model_path": {
            "type": "str",
            "default": "../../vendor/layerModels/folderRepos/arabic_models/m2m100-418M"
          },
          "load_weights": {
            "type": "bool",
            "default": true
          }
        },
        "outputs": {
          "model": "M2M100Model",
          "weight_coverage": "float"
        }
      }
    },
    {
      "id": "rlm_recursive_learner",
      "type": "CustomComponent",
      "name": "RLM Recursive Learning Engine",
      "position": {"x": 400, "y": 200},
      "data": {
        "component": "RLMRecursiveLearner",
        "description": "Recursive learning with self-improvement loops",
        "inputs": {
          "rlm_path": {
            "type": "str",
            "default": "../../vendor/layerModels/folderRepos/rlm"
          },
          "models": "Model[]",
          "learning_depth": {
            "type": "int",
            "default": 3,
            "description": "Number of recursive learning iterations"
          },
          "improvement_threshold": {
            "type": "float",
            "default": 0.01,
            "description": "Min improvement to continue recursion"
          },
          "meta_learning_rate": {
            "type": "float",
            "default": 0.001
          }
        },
        "outputs": {
          "improved_models": "Model[]",
          "learning_history": "dict",
          "improvement_metrics": "dict"
        }
      }
    },
    {
      "id": "toolorchestra_intelligence",
      "type": "CustomComponent",
      "name": "ToolOrchestra Intelligence Layer",
      "position": {"x": 400, "y": 400},
      "data": {
        "component": "ToolOrchestraIntelligence",
        "description": "Intelligent tool selection and orchestration for optimal training",
        "inputs": {
          "toolorchestra_path": {
            "type": "str",
            "default": "../../vendor/layerModels/folderRepos/toolorchestra"
          },
          "training_context": "dict",
          "available_tools": {
            "type": "list",
            "default": [
              "data_augmentation",
              "curriculum_learning",
              "adaptive_lr",
              "ensemble_distillation",
              "error_analysis",
              "active_learning"
            ]
          }
        },
        "outputs": {
          "recommended_tools": "Tool[]",
          "training_strategy": "dict",
          "optimization_plan": "dict"
        }
      }
    },
    {
      "id": "model_comparator",
      "type": "CustomComponent",
      "name": "Multi-Model Comparator",
      "position": {"x": 700, "y": 100},
      "data": {
        "component": "ModelComparator",
        "description": "Compare multiple models on same test set",
        "inputs": {
          "models": "Model[]",
          "test_data": "DataBatch[]",
          "metrics": {
            "type": "list",
            "default": ["BLEU", "Accuracy", "Latency", "Memory"]
          }
        },
        "outputs": {
          "comparison_results": "ComparisonResults",
          "best_model": "Model",
          "recommendations": "dict"
        }
      }
    },
    {
      "id": "ensemble_trainer",
      "type": "CustomComponent",
      "name": "Ensemble Training Coordinator",
      "position": {"x": 700, "y": 250},
      "data": {
        "component": "EnsembleTrainer",
        "description": "Train ensemble of M2M100 + Tencent with intelligent weighting",
        "inputs": {
          "models": "Model[]",
          "training_data": "DataBatch[]",
          "ensemble_method": {
            "type": "str",
            "options": ["weighted_average", "stacking", "boosting", "voting"],
            "default": "weighted_average"
          },
          "weight_optimization": {
            "type": "str",
            "options": ["grid_search", "bayesian", "evolutionary"],
            "default": "bayesian"
          }
        },
        "outputs": {
          "ensemble_model": "EnsembleModel",
          "optimal_weights": "dict",
          "ensemble_metrics": "dict"
        }
      }
    },
    {
      "id": "curriculum_designer",
      "type": "CustomComponent",
      "name": "Intelligent Curriculum Designer",
      "position": {"x": 700, "y": 400},
      "data": {
        "component": "CurriculumDesigner",
        "description": "Design optimal training curriculum using ToolOrchestra intelligence",
        "inputs": {
          "training_data": "DataBatch[]",
          "model_capability": "dict",
          "difficulty_estimator": "function",
          "curriculum_strategy": {
            "type": "str",
            "options": ["easy_to_hard", "hard_to_easy", "adaptive", "competence"],
            "default": "adaptive"
          }
        },
        "outputs": {
          "curriculum": "TrainingCurriculum",
          "batch_schedule": "dict",
          "expected_improvement": "float"
        }
      }
    },
    {
      "id": "recursive_fine_tuner",
      "type": "CustomComponent",
      "name": "RLM Recursive Fine-tuner",
      "position": {"x": 1000, "y": 200},
      "data": {
        "component": "RecursiveFineTuner",
        "description": "Recursive self-improvement loop with RLM",
        "inputs": {
          "base_model": "Model",
          "training_data": "DataBatch[]",
          "rlm_config": "dict",
          "max_recursion_depth": {
            "type": "int",
            "default": 5
          },
          "convergence_threshold": {
            "type": "float",
            "default": 0.001
          }
        },
        "outputs": {
          "fine_tuned_model": "Model",
          "recursion_history": "dict[]",
          "convergence_metrics": "dict"
        }
      }
    },
    {
      "id": "intelligent_augmenter",
      "type": "CustomComponent",
      "name": "ToolOrchestra Data Augmenter",
      "position": {"x": 1000, "y": 400},
      "data": {
        "component": "IntelligentAugmenter",
        "description": "Intelligent data augmentation using ToolOrchestra",
        "inputs": {
          "training_data": "DataBatch[]",
          "augmentation_tools": "Tool[]",
          "target_size": {
            "type": "int",
            "default": 100000
          },
          "quality_threshold": {
            "type": "float",
            "default": 0.9
          }
        },
        "outputs": {
          "augmented_data": "DataBatch[]",
          "augmentation_stats": "dict",
          "quality_distribution": "dict"
        }
      }
    },
    {
      "id": "meta_optimizer",
      "type": "CustomComponent",
      "name": "Meta-Learning Optimizer",
      "position": {"x": 1300, "y": 200},
      "data": {
        "component": "MetaOptimizer",
        "description": "Learn to learn - optimize training process itself",
        "inputs": {
          "training_history": "dict[]",
          "model_performance": "dict[]",
          "optimization_target": {
            "type": "str",
            "options": ["bleu", "latency", "balanced"],
            "default": "balanced"
          }
        },
        "outputs": {
          "optimal_hyperparameters": "dict",
          "training_recipe": "dict",
          "expected_performance": "dict"
        }
      }
    },
    {
      "id": "performance_predictor",
      "type": "CustomComponent",
      "name": "Performance Predictor",
      "position": {"x": 1300, "y": 400},
      "data": {
        "component": "PerformancePredictor",
        "description": "Predict final performance before full training",
        "inputs": {
          "model": "Model",
          "sample_data": "DataBatch[]",
          "training_config": "dict",
          "historical_data": "dict[]"
        },
        "outputs": {
          "predicted_bleu": "float",
          "predicted_latency": "float",
          "confidence_interval": "dict",
          "training_cost_estimate": "float"
        }
      }
    }
  ],
  "edges": [
    {
      "source": "model_selector",
      "sourceHandle": "model_configs",
      "target": "tencent_hy_mt",
      "targetHandle": "config"
    },
    {
      "source": "model_selector",
      "sourceHandle": "model_configs",
      "target": "m2m100_loader",
      "targetHandle": "config"
    },
    {
      "source": "tencent_hy_mt",
      "sourceHandle": "model",
      "target": "rlm_recursive_learner",
      "targetHandle": "models"
    },
    {
      "source": "m2m100_loader",
      "sourceHandle": "model",
      "target": "rlm_recursive_learner",
      "targetHandle": "models"
    },
    {
      "source": "rlm_recursive_learner",
      "sourceHandle": "improved_models",
      "target": "model_comparator",
      "targetHandle": "models"
    },
    {
      "source": "toolorchestra_intelligence",
      "sourceHandle": "recommended_tools",
      "target": "curriculum_designer",
      "targetHandle": "tools"
    },
    {
      "source": "toolorchestra_intelligence",
      "sourceHandle": "training_strategy",
      "target": "ensemble_trainer",
      "targetHandle": "strategy"
    },
    {
      "source": "model_comparator",
      "sourceHandle": "best_model",
      "target": "recursive_fine_tuner",
      "targetHandle": "base_model"
    },
    {
      "source": "curriculum_designer",
      "sourceHandle": "curriculum",
      "target": "recursive_fine_tuner",
      "targetHandle": "curriculum"
    },
    {
      "source": "intelligent_augmenter",
      "sourceHandle": "augmented_data",
      "target": "recursive_fine_tuner",
      "targetHandle": "training_data"
    },
    {
      "source": "recursive_fine_tuner",
      "sourceHandle": "fine_tuned_model",
      "target": "meta_optimizer",
      "targetHandle": "model"
    },
    {
      "source": "recursive_fine_tuner",
      "sourceHandle": "recursion_history",
      "target": "performance_predictor",
      "targetHandle": "historical_data"
    }
  ],
  "metadata": {
    "author": "AI Nucleus Team",
    "created": "2026-01-10",
    "description": "Intelligent multi-model training with Tencent HY-MT, M2M100, RLM recursive learning, and ToolOrchestra intelligence",
    "tags": ["multi-model", "recursive-learning", "intelligent-training", "ensemble", "meta-learning"],
    "capabilities": [
      "Multi-model comparison (M2M100 vs Tencent HY-MT)",
      "RLM recursive self-improvement",
      "ToolOrchestra intelligent orchestration",
      "Ensemble training with optimal weighting",
      "Curriculum learning",
      "Intelligent data augmentation",
      "Meta-learning optimization",
      "Performance prediction"
    ]
  }
}
