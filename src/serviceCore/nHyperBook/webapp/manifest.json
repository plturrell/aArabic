{
    "_version": "1.59.0",
    "sap.app": {
        "id": "hypershimmy",
        "type": "application",
        "i18n": "i18n/i18n.properties",
        "applicationVersion": {
            "version": "0.1.0"
        },
        "title": "{{appTitle}}",
        "description": "{{appDescription}}",
        "resources": "resources.json",
        "dataSources": {
            "mainService": {
                "uri": "/odata/v4/research/",
                "type": "OData",
                "settings": {
                    "odataVersion": "4.0",
                    "localUri": "localService/metadata.xml"
                }
            }
        }
    },
    "sap.ui": {
        "technology": "UI5",
        "icons": {
            "icon": "",
            "favIcon": "",
            "phone": "",
            "phone@2": "",
            "tablet": "",
            "tablet@2": ""
        },
        "deviceTypes": {
            "desktop": true,
            "tablet": true,
            "phone": true
        }
    },
    "sap.ui5": {
        "flexEnabled": true,
        "dependencies": {
            "minUI5Version": "1.120.0",
            "libs": {
                "sap.m": {},
                "sap.ui.core": {},
                "sap.f": {},
                "sap.ui.layout": {}
            }
        },
        "contentDensities": {
            "compact": true,
            "cozy": true
        },
        "models": {
            "i18n": {
                "type": "sap.ui.model.resource.ResourceModel",
                "settings": {
                    "bundleName": "hypershimmy.i18n.i18n"
                }
            },
            "": {
                "dataSource": "mainService",
                "preload": true,
                "settings": {
                    "synchronizationMode": "None",
                    "operationMode": "Server",
                    "autoExpandSelect": true,
                    "earlyRequests": true
                }
            }
        },
        "resources": {
            "css": [
                {
                    "uri": "css/style.css"
                }
            ]
        },
        "routing": {
            "config": {
                "routerClass": "sap.f.routing.Router",
                "viewType": "XML",
                "async": true,
                "viewPath": "hypershimmy.view",
                "controlAggregation": "beginColumnPages",
                "controlId": "layout",
                "clearControlAggregation": false
            },
            "routes": [
                {
                    "name": "main",
                    "pattern": "",
                    "target": [
                        "master"
                    ]
                },
                {
                    "name": "detail",
                    "pattern": "sources/{sourceId}",
                    "target": [
                        "master",
                        "detail"
                    ]
                },
                {
                    "name": "chat",
                    "pattern": "sources/{sourceId}/chat",
                    "target": [
                        "master",
                        "detail",
                        "chat"
                    ]
                },
                {
                    "name": "summary",
                    "pattern": "sources/{sourceId}/summary",
                    "target": [
                        "master",
                        "detail",
                        "summary"
                    ]
                },
                {
                    "name": "audio",
                    "pattern": "sources/{sourceId}/audio",
                    "target": [
                        "master",
                        "detail",
                        "audio"
                    ]
                },
                {
                    "name": "slides",
                    "pattern": "sources/{sourceId}/slides",
                    "target": [
                        "master",
                        "detail",
                        "slides"
                    ]
                }
            ],
            "targets": {
                "master": {
                    "viewName": "Master",
                    "controlAggregation": "beginColumnPages",
                    "viewLevel": 1
                },
                "detail": {
                    "viewName": "Detail",
                    "controlAggregation": "midColumnPages",
                    "viewLevel": 2
                },
                "chat": {
                    "viewName": "Chat",
                    "controlAggregation": "endColumnPages",
                    "viewLevel": 3
                },
                "summary": {
                    "viewName": "Summary",
                    "controlAggregation": "endColumnPages",
                    "viewLevel": 3
                },
                "audio": {
                    "viewName": "Audio",
                    "controlAggregation": "endColumnPages",
                    "viewLevel": 3
                },
                "slides": {
                    "viewName": "Slides",
                    "controlAggregation": "endColumnPages",
                    "viewLevel": 3
                }
            }
        },
        "rootView": {
            "viewName": "hypershimmy.view.App",
            "type": "XML",
            "async": true,
            "id": "app"
        }
    }
}
