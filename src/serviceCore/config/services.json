{
  "services": {
    "shimmy": {
      "name": "Shimmy AI Workflow Engine",
      "url": "http://shimmy:3001",
      "health_endpoint": "/health",
      "version": "1.0.0",
      "metadata": {
        "type": "workflow_engine",
        "vendor": "shimmy-ai",
        "capabilities": [
          "workflow_execution",
          "model_management", 
          "streaming",
          "tool_execution",
          "a2ui_generation"
        ],
        "endpoints": {
          "models": "/api/models",
          "tools": "/api/tools",
          "workflows": "/api/workflows/execute",
          "websocket": "/ws/generate"
        }
      }
    },
    "memgraph": {
      "name": "Memgraph Graph Database",
      "url": "bolt://memgraph:7687",
      "health_endpoint": "bolt://memgraph:7687",
      "version": "2.11.0",
      "metadata": {
        "type": "graph_database",
        "vendor": "memgraph",
        "capabilities": [
          "graph_storage",
          "cypher_queries",
          "analytics",
          "workflow_tracking",
          "dependency_management"
        ],
        "protocols": ["bolt", "http"],
        "http_endpoint": "http://memgraph-lab:3000"
      }
    },
    "a2ui": {
      "name": "A2UI Dynamic Interface Generator",
      "url": "http://backend:8000",
      "health_endpoint": "/api/a2ui/health",
      "version": "0.9.0",
      "metadata": {
        "type": "ui_generator",
        "vendor": "google",
        "capabilities": [
          "dynamic_ui",
          "component_rendering",
          "data_binding"
        ],
        "endpoints": {
          "generate": "/api/a2ui/generate",
          "workflow_result": "/api/a2ui/workflow-result"
        }
      }
    },
    "marquez": {
      "name": "Marquez Lineage Service",
      "url": "http://marquez:5000",
      "health_endpoint": "/api/v1/namespaces",
      "version": "0.50.0",
      "metadata": {
        "type": "lineage_service",
        "vendor": "marquez",
        "capabilities": [
          "dataset_catalog",
          "job_run_tracking",
          "openlineage_api",
          "graph_visualization"
        ],
        "admin_endpoint": "http://marquez:5001",
        "web_ui": "http://marquez-web:3000",
        "api_docs": "http://marquez:5000/api/v1/openapi.json",
        "docker": {
          "script": "./vendor/marquez-run.sh up",
          "compose_file": "./vendor/marquez/docker-compose.yml"
        }
      }
    },
    "toolorchestra": {
      "name": "ToolOrchestra RL Framework",
      "url": "http://backend:8000",
      "health_endpoint": "/api/orchestration/health",
      "version": "1.2.0",
      "metadata": {
        "type": "rl_orchestrator",
        "vendor": "nvidia",
        "capabilities": [
          "reinforcement_learning",
          "tool_optimization",
          "strategy_selection",
          "performance_analytics",
          "adaptive_orchestration"
        ],
        "endpoints": {
          "orchestrate": "/api/orchestration/orchestrate",
          "optimize": "/api/orchestration/optimize",
          "analytics": "/api/orchestration/analytics",
          "strategies": "/api/orchestration/strategies"
        },
        "strategies": [
          "SEQUENTIAL",
          "PARALLEL", 
          "ADAPTIVE",
          "RL_OPTIMIZED"
        ]
      }
    },
    "open_canvas": {
      "name": "Open Canvas UI Framework",
      "url": "http://opencanvas:3000",
      "health_endpoint": "/api/health",
      "version": "2.1.0",
      "metadata": {
        "type": "ui_framework",
        "vendor": "langchain",
        "capabilities": [
          "collaborative_ui",
          "agent_interaction",
          "workflow_visualization",
          "real_time_updates"
        ],
        "endpoints": {
          "canvas": "/api/canvas",
          "components": "/api/components",
          "websocket": "/ws/canvas"
        }
      }
    },
    "nucleus_flow": {
      "name": "NucleusFlow AI Builder",
      "url": "http://langflow:7860",
      "health_endpoint": "/health",
      "version": "1.0.0",
      "metadata": {
        "type": "low_code_builder",
        "vendor": "langflow",
        "capabilities": [
          "visual_flow_building",
          "component_chaining",
          "llm_orchestration",
          "custom_components"
        ],
        "endpoints": {
          "flows": "/api/v1/flows",
          "components": "/api/v1/components",
          "predict": "/api/v1/predict"
        }
      }
    },
    "n8n": {
      "name": "n8n Workflow Automation",
      "url": "http://n8n:5678",
      "health_endpoint": "/healthz",
      "version": "latest",
      "metadata": {
        "type": "workflow_automation",
        "vendor": "n8n",
        "capabilities": [
          "workflow_execution",
          "webhooks",
          "integrations",
          "scheduling"
        ],
        "endpoints": {
          "health": "/healthz",
          "webhook": "/webhook",
          "api": "/api/v1"
        }
      }
    },
    "qdrant": {
      "name": "Qdrant Vector Database",
      "url": "http://qdrant:6333",
      "health_endpoint": "/readyz",
      "version": "1.7.0",
      "metadata": {
        "type": "vector_database",
        "vendor": "qdrant",
        "capabilities": [
          "vector_similarity_search",
          "semantic_search",
          "embedding_storage",
          "filtering",
          "clustering",
          "recommendation_engine"
        ],
        "endpoints": {
          "collections": "/collections",
          "points": "/collections/{collection_name}/points",
          "search": "/collections/{collection_name}/points/search",
          "grpc": "qdrant:6334"
        },
        "protocols": ["http", "grpc"],
        "collections": [
          "workflows",
          "documents",
          "invoices",
          "tools",
          "a2ui_components"
        ],
        "vector_dimensions": {
          "workflows": 768,
          "documents": 1536,
          "invoices": 768,
          "tools": 384,
          "a2ui_components": 512
        }
      }
    }
  },
  "discovery": {
    "health_check_interval": 30,
    "health_check_timeout": 5,
    "max_retries": 3,
    "retry_delay": 1000
  },
  "load_balancing": {
    "strategy": "round_robin",
    "health_check_required": true
  },
  "monitoring": {
    "metrics_enabled": true,
    "logging_level": "INFO",
    "alert_thresholds": {
      "response_time": 5.0,
      "error_rate": 0.1,
      "availability": 0.95
    }
  }
}
